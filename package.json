    "lodash.debounce": "^4.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "redis": "^4.0.4",
    "regenerator-runtime": "^0.13.9",
    "serialize-javascript": "^6.0.0",
    "uuid": "^8.3.2",
